    </section>


    <!--==========================
      Schedule Section
    ============================-->
    <section id="schedule" class="section-with-bg">
      <div class="container wow">
        <div class="section-header">
          <h2>Upcoming Events</h2>
          <p>Here is a list of upcoming events</p>
        </div>


        <h3 class="sub-heading">Here's a list of upcoming events in various cities. Events are organised based on date of listing</h3>

        <div class="tab-content row justify-content-center">

          <!-- Schdule Day 1 -->
          <div role="tabpanel" class="col-lg-9 tab-pane fade show active" id="day-1">
            <% data.forEach(function(ele ){  %>


        <a href="event/eventdisc/<%=ele.eid %>">  <div class="row schedule-item">
              <div class="col-md-2" style="color:black"><%=ele.edate.toDateString() %></div>
              <div class="col-md-10">
      <!--          <div class="speaker">
                  <img src="img/speakers/1.jpg" alt="Brenden Legros">
                </div> <-->
                <h4><%=ele.ename %></h4>
                <p><%=ele.venue%></p>
              </div>
            </div></a>
        <%    });  %>



          </div>
          <!-- End Schdule Day 1 -->


        </div>

      </div>

    </section>





